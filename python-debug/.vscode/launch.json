{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: ROS Node",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/kineval_ros_ws/src/kineval_ros_pkg/scripts/kineval_node.py",
            "console": "integratedTerminal",
            "env": {
                "ROS_MASTER_URI": "http://localhost:11311",
